{
  "name": "kubesquash",
  "displayName": "kubesquash",
  "description": "Debug you micro-service via kubesquash using vscode UI",
  "version": "0.0.1",
  "publisher": "ilevine",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.debugPod"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.debugPod",
        "title": "Kubesquash - debug pod"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/shelljs": "^0.7.9",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
